<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fgwater.frame.mapper.wbb.CqEvaRateMapper">

	<select id="query" parameterType="HashMap" resultType="HashMap">
		select id,temperature,rate from t_wbb_cq_eva_rate order by
		temperature asc
	</select>

	<delete id="deleteAll">delete t_wbb_cq_eva_rate</delete>

</mapper>